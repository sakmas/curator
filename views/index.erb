<% @feeds.each do |feed| %>
  <table class="table table-hover">
    <thead>
      <tr><th><%= feed.title %></th></tr>
    </thead>
    <tbody>
        <% feed.entries.each do |entry| %>
          <tr><td><a href="<%= entry.url %>" target="new"><%= entry.title %></td></tr>
        <% end %>
    </tbody>
  </table>
<% end %>
