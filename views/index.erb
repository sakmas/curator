<% @feeds.each do |feed| %>
  <table class="table table-bordered">
    <thead>
      <tr><th><%= feed.title %></th></tr>
    </thead>
    <tbody>
        <% feed.entries.each do |entry| %>
          <tr><td>
            <small class="text-muted"><%= entry.published.getlocal().strftime("%m/%d %H:%M") %></small><br>
            <a href="<%= entry.url %>" target="new"><%= entry.title %>
          </td></tr>
        <% end %>
    </tbody>
  </table>
<% end %>
